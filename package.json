{
  "name": "devdas",
  "displayName": "Devdas: Twitter Interaction Reminder",
  "description": "A VSCode extension to remind you to reply and like your girl's tweets",
  "version": "0.0.3",
  "publisher": "AdithyaKris",
  "engines": {
    "vscode": "^1.94.0"
  },
  "author": {
    "name": "Adithya Krishna",    
    "email": "aadithya794@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adithyaakrishna/devdas.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:devdasTwitterReminder"
  ],
  "main": "./dist/extension.js",
  "icon": "media/ribbon.png",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "devdas-twitter-reminder",
          "title": "Devdas",
          "icon": "media/ribbon.png"
        }
      ]
    },
    "views": {
      "devdas-twitter-reminder": [
        {
          "type": "webview",
          "id": "devdasTwitterReminder",
          "name": "Dont Forget to Text Your Girl"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "^16.x",
    "@types/vscode": "^1.94.0",
    "typescript": "^4.x"
  }
}
